<%- include('includes/head')%>
	<body>
		<%- include('includes/nav')%>

		<div class="mt-2 container">
			<!-- Page Layout here -->
		
			<div class="row">
				<!-- <div class="col s12 add-user-column">
					<a href="/new" class="btn">Add user </a>
				</div> -->
			</div>

			<div class="row ">
				<div class="col m12 s12 users">
					<div class="card">
						<table class="highlight responsive-table">
							<thead>
							  <tr>
								  <th>ID</th>
								  <th>Name</th>
								  <th>Email</th>
								  <th>Created At</th>
								  <th>User level</th>
							
							  </tr>
							</thead>
					
							<tbody>
								<% for (let person in users) { %>
								
								<tr>
									<td><%= users[person].id %></td>
									<td><a href="/show/<%= users[person].id %>"><%= users[person].full_name %></a></td>
									<td><%= users[person].email %></td>
									<td><%= users[person].created_at %></td>
									<td><%= (users[person].user_level == 1) ? "Normal" : "Admin" %></td>
									
								</tr>
								<% } %>
							
							  
							</tbody>
						</table>
					</div>
				</div>

			
			</div>
		</div>

		<script></script>

